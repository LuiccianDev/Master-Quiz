---
interface Props {
  value: number;
  max: number;
}
const { value, max } = Astro.props;
const percent = Math.round((value / max) * 100);
---
<div class="w-full bg-gray-200 rounded-full h-2">
  <div
    class="bg-primary-600 h-2 rounded-full transition-all"
    style={`width: ${percent}%`}
  ></div>
</div>
